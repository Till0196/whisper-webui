# FOR DEVELOPMENT

# APIエンドポイント
VITE_WHISPER_API_URL=https://your-api-endpoint.com
VITE_HEALTH_CHECK_URL=http://your-api-endpoint.com/health

# APIキー
VITE_WHISPER_API_TOKEN=your_api_key_here

# Whisperサーバープロキシモードの設定の設定
VITE_USE_SERVER_PROXY=true
VITE_SERVER_PROXY_URL=http://localhost:3001
VITE_WHISPER_PROXY_TOKEN=your_api_key_here

# クレデンシャル情報の隠蔽設定
VITE_HIDE_CREDENTIALS=false
VITE_ALLOW_CREDENTIAL_EDIT=false

# FOR PRODUCTION

# APIエンドポイント
WHISPER_API_URL=https://your-api-endpoint.com
HEALTH_CHECK_URL=http://your-api-endpoint.com/health

# APIキー
WHISPER_API_TOKEN=your_api_key_here

# Whisperサーバープロキシモードの設定
USE_SERVER_PROXY=true
SERVER_PROXY_URL=http://localhost:3001
WHISPER_PROXY_TOKEN=your_api_key_here

# クレデンシャル情報の隠蔽設定
HIDE_CREDENTIALS=false
ALLOW_CREDENTIAL_EDIT=false

